node n0 {
    type lanswitch
    network-config {
	hostname switch1
	!
    }
    canvas c0
    iconcoords {408 72}
    labelcoords {408 96}
    interface-peer {e0 n5}
    interface-peer {e1 n1}
    interface-peer {e2 n6}
}

node n1 {
    type pc
    network-config {
	hostname pc-vizinho
	!
	interface eth0
	 ip address 10.0.0.20/24
	 ipv6 address fc00::20/64
	 mac address 42:00:aa:00:00:04
	!
	interface lo0
	 type lo
	 ip address 127.0.0.1/24
	 ipv6 address ::1/128
	!
	ip route 0.0.0.0/0 10.0.0.1
	!
	ipv6 route ::/0 fc00::1
	!
    }
    canvas c0
    iconcoords {216 72}
    labelcoords {216 103}
    interface-peer {eth0 n0}
    services {ssh tcpdump}
}

