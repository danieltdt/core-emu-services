node n6 {
    type lanswitch
    network-config {
	hostname switch
	!
    }
    canvas c1
    iconcoords {218.0 328.0}
    labelcoords {218.0 352.0}
    interface-peer {e0 n3}
    interface-peer {e1 n4}
    interface-peer {e3 n1}
}

link l1 {
    nodes {n2 n3}
}

link l3 {
    nodes {n6 n3}
    bandwidth 0
}

link l4 {
    nodes {n6 n4}
    bandwidth 0
}

link l2 {
    nodes {n6 n1}
    bandwidth 0
}

link l6 {
    nodes {n3 n5}
    bandwidth 0
}

annotation a2 {
    type text
    iconcoords {68.0 263.0}
    label {}
    canvas c1
}

canvas c1 {
    name {Sandnet}
}

option global {
    interface_names yes
    ip_addresses yes
    ipv6_addresses yes
    node_labels yes
    link_labels yes
    show_api no
    background_images no
    annotations yes
    grid yes
    traffic_start 0
}

option session {
    controlnet=
    enablerj45=1
    enablesdt=0
    preservedir=1
}

