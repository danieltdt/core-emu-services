node n1 {
    type router
    model host
    network-config {
	hostname http
	!
	interface eth0
	 ip address 198.17.231.31/24
	 ipv6 address a:3::12/64
	!
    }
    canvas c1
    iconcoords {334.0 454.0}
    labelcoords {334.0 486.0}
    services {DefaultRoute SSH SandnetHTTP}
    interface-peer {eth0 n6}
}

node n2 {
    type router
    model PC
    network-config {
	hostname vizinho
	!
	interface eth0
	 ip address 192.80.209.11/24
	 ipv6 address a:1::20/64
	!
    }
    canvas c1
    iconcoords {376.0 79.0}
    labelcoords {376.0 111.0}
    interface-peer {eth0 n3}
}

node n3 {
    type router
    model router
    network-config {
	hostname roteador
	!
	interface eth1
	 ip address 200.0.70.1/24
	 ipv6 address a:2::1/64
	!
	interface eth2
	 ip address 198.17.231.1/24
	 ipv6 address a:3::1/64
	!
	interface eth0
	 ip address 192.80.209.1/24
	 ipv6 address a:1::1/64
	!
    }
    canvas c1
    iconcoords {373.0 255.0}
    labelcoords {373.0 287.0}
    interface-peer {eth0 n2}
    interface-peer {eth2 n6}
    interface-peer {eth1 n5}
}

