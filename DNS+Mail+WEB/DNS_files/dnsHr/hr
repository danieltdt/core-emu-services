; Tu cu ubaciti SOA
;
$TTL 60000
@ IN SOA dnsHr.hr. root.dnsHr.hr (
        2002102801 ; serial
        28 ; refresh
        14 ; retry
        3600000 ; expire
        0 ; default_ttl
)

@                   IN  NS   dnsHr.hr.
@                   IN  NS   hr2.com.
dnsHr.hr.           IN  A    7.0.0.2
hr2.com.            IN  A    4.0.0.2

fer.hr.             IN  NS   dnsFer.fer.hr.
dnsFer.fer.hr.      IN  A    8.0.0.2

