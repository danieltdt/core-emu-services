;
; BIND reverse data file for sandnet interfaces
;
$TTL	604800
@	IN	SOA	dns.sandnet. root.sandnet. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
;@	IN	NS	dns.
@	IN	NS	dns.sandnet.

; other nodes
31	IN	PTR	http.sandnet.
41	IN	PTR	smtp.sandnet.
